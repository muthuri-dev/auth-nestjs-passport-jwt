generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @default(cuid())
  user_name     String   @unique
  email         String?  @unique
  password      String
  refresh_token String?  @db.LongText
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt

  @@map("users")
}
